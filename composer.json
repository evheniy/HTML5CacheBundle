    "keywords": ["HTML5", "Cache", "Symfony", "HTML5 Application Cache", "Cache Manifest"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name":     "evheniy",
            "homepage": "https://github.com/evheniy"
        }
    ],
    "require": {
        "php":             ">=5.3.0",
        "symfony/symfony": ">=2.3",
        "evheniy/jquery-bundle": ">=1.0"
    },
    "require-dev": {
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Evheniy\\HTML5CacheBundle": ""
        }
    },
    "target-dir" : "Evheniy/HTML5CacheBundle"
